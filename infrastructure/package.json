{
  "name": "invoice-scanner-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for serverless invoice scanner",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "clean": "rm -rf lib/ cdk.out/ node_modules/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.95.0",
    "constructs": "^10.3.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "serverless",
    "invoice-scanner"
  ]
}
